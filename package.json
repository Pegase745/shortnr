{
  "name": "shortnr",
  "version": "1.0.0-alpha.8",
  "description": "Simple URL shortener written in TypeScript and backed by Redis.",
  "author": "Michel Nemnom",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:Pegase745/shortnr.git"
  },
  "bin": {
    "shortnr": "./bin/shortnr.js"
  },
  "engines": {
    "node": "^8 || ^10"
  },
  "files": [
    "/dist",
    "/bin/shortnr.js"
  ],
  "scripts": {
    "start": "concurrently npm:start:*",
    "start:dev": "cross-env NODE_ENV=development webpack-dev-server",
    "start:server": "cross-env tsnd --respawn --transpileOnly src/server",
    "build:dev": "npm run clean && cross-env NODE_ENV=development webpack --config webpack.config.js",
    "build:prod": "npm run clean && cross-env NODE_ENV=production webpack --config webpack.config.js && npm run build:server",
    "build:server": "cross-env tsc --project tsconfig.server.json",
    "serve": "npm run build:prod && NODE_ENV=production node dist/server/index.js",
    "clean": "rimraf dist",
    "lint": "tslint --project tsconfig.json",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "test": "jest --verbose",
    "coverage": "jest --coverage"
  },
  "devDependencies": {
    "@types/config": "0.0.34",
    "@types/enzyme": "^3.9.0",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/ioredis": "^4.0.9",
    "@types/jest": "^24.0.11",
    "@types/node": "^10.12.26",
    "@types/reach__router": "^1.2.2",
    "@types/react": "^16.8.7",
    "@types/react-copy-to-clipboard": "^4.2.6",
    "@types/react-dom": "^16.8.2",
    "@types/uniqid": "^4.1.3",
    "@types/webpack-env": "^1.13.9",
    "awesome-typescript-loader": "^5.2.1",
    "brotli-webpack-plugin": "^1.1.0",
    "compression-webpack-plugin": "^2.0.0",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "fastify-blipp": "^1.2.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.3.1",
    "jest-enzyme": "^7.0.2",
    "lint-staged": "^8.1.5",
    "node-sass": "^4.11.0",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.3",
    "ts-jest": "^24.0.0",
    "ts-node-dev": "^1.0.0-pre.32",
    "tslint": "^5.13.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "@reach/router": "^1.2.1",
    "axios": "^0.18.0",
    "config": "^3.0.1",
    "fastify": "^2.0.1",
    "fastify-helmet": "^3.0.0",
    "fastify-no-icon": "^3.0.0",
    "fastify-plugin": "^1.5.0",
    "fastify-redis": "^3.0.0",
    "fastify-static": "^2.3.4",
    "pino-pretty": "^2.5.0",
    "react": "^16.8.4",
    "react-bulma-components": "^3.0.1-3",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.8.4",
    "serve-static": "^1.13.2",
    "uuid-by-string": "^2.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
